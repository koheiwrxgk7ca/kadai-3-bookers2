<%= render 'books/list', user: @user %>

<h1>Users</h1>
<table>
<thead>
<tr>
 <th><h4>image</h4></th>
 <th><h4>name</h4></th>
</tr>
</thead>

<% @users.each do |user| %>
<tbody>
<tr>
   <td>
     <%= image_tag user.get_profile_image(100,100) %>
   </td>
   <td>
      <%= user.name  %>
   </td>
   <td>
      <%= link_to "show", user_path(user.id) %>
   </td>
</tr>
</tbody>
<% end %>
</table>
